{
  "name": "electron-twitch-chat",
  "version": "1.0.0",
  "description": "A Twitch Chat client built with Electron and Angular",
  "main": "main.js",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"electron .\" ",
    "run": "electron .",
    "compile": "\"npm run tsc:w\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "repository": "https://github.com/prasoc/electron-twitch-chat",
  "keywords": [
    "ElectronChat"
  ],
  "author": "prasoc",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.4.1",
    "typescript": "^2.0.2",
    "typings": "^1.3.2",
    "concurrently": "^2.2.0"
  },
  "dependencies": {
    "@angular/common": "~2.2.0",
    "@angular/compiler": "~2.2.0",
    "@angular/core": "~2.2.0",
    "@angular/forms": "~2.2.0",
    "@angular/http": "~2.2.0",
    "@angular/platform-browser": "~2.2.0",
    "@angular/platform-browser-dynamic": "~2.2.0",
    "@angular/router": "~3.2.0",
    "core-js": "^2.4.1",
    "jquery": "^3.1.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "socket.io-client": "^1.4.8",
    "socketio": "^1.0.0",
    "systemjs": "0.19.40",
    "twitch-sdk": "^0.3.1",
    "zone.js": "^0.6.26"
  }
}
